
public class ExercicioFatorial {

	public static void main(String[] args) {
		int total = 1;
		for (int contador = 1; contador <= 10; contador++) {
			total = total * contador;
			System.out.print("x" + contador);
			if(contador >= 10) {
				System.out.print("=");
				System.out.print(total);	
			}
			
			System.out.println();
		}
	}
}
